                {
                    "datasource": {
                        "uid": "$datasource"
                    },
                    "expr": "node_uname_info{job=~\"$job\",instance=~\"$instance\"}",
                    "format": "table",
                    "instant": true,
                    "intervalFactor": 1,
                    "refId": "OS"
                },
                {
                    "datasource": {
                        "uid": "$datasource"
                    },
                    "expr": "time() - node_boot_time_seconds{job=~\"$job\",instance=~\"$instance\"}",
                    "format": "table",
                    "instant": true,
                    "intervalFactor": 1,
                    "refId": "UPTIME"
                },
                {
                    "datasource": {
                        "uid": "$datasource"
                    },
                    "expr": "avg by (instance) (node_load1{job=~\"$job\",instance=~\"$instance\"})",
                    "format": "table",
                    "instant": true,
                    "intervalFactor": 1,
                    "refId": "LOAD1"
                },
                {
                    "datasource": {
                        "uid": "$datasource"
                    },
